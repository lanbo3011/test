<driver name="NULL">
  <global_def>
    <var_seq name="GpiConfigFile" type=DALPROP_DATA_TYPE_STRUCT_INC_FILE>
      ./../GPI/core/gpi_cfg_sdm670.c
    </var_seq>
  </global_def>
  
  <device id="/core/hwengines/gpi">
  
    <props name="tgt_num_gpii" type=DALPROP_ATTR_TYPE_UINT32>
      8
    </props>
    
    <props name="tgt_gpi_config" type=DALPROP_ATTR_TYPE_STRUCT_PTR>
      tgt_gpi_config
    </props>
    
    <props name="tgt_gpii_config" type=DALPROP_ATTR_TYPE_STRUCT_PTR>
      tgt_gpii_config
    </props>
    
  </device>

</driver>

